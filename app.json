{
  "expo": {
    "owner": "vehemgroup",
    "name": "strathSpace",
    "slug": "strathSpace",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "strathspace",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.strathspace.mobile",
      "buildNumber": "16",
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Strathspace needs camera access so you can take photos for your  profile. Your photos help other students see who you are and make meaningful connections on campus.",
        "NSPhotoLibraryUsageDescription": "Strathspace needs access to your photo library so you can select and upload photos to your  profile. Choose your best photos to showcase your personality to other students.",
        "NSPhotoLibraryAddUsageDescription": "Strathspace needs permission to save photos to your library when you want to keep images from conversations or your profile."
      }
    },
    "android": {
      "package": "com.strathspace.mobile",
      "versionCode": 4,
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#7B4397",
          "dark": {
            "image": "./assets/images/splash.png",
            "backgroundColor": "#7B4397"
          }
        }
      ],
      "expo-secure-store",
      "expo-web-browser",
      "@react-native-community/datetimepicker",
      "expo-font",
      "expo-apple-authentication"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "72c4aded-f0d8-4049-95fa-401be27ae5fb"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/72c4aded-f0d8-4049-95fa-401be27ae5fb"
    }
  }
}
